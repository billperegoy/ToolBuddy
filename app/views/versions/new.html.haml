.container
  .page-header
    %h1 New Tool Version

  = render partial: 'common/form_errors', locals: {object: @version}
  = form_for([@tool, @version]) do |f|
    %p
      = f.label :name
      = f.text_field :name
    %p
      = f.label :release_notes
      = f.text_area :release_notes
    %p
      = f.submit
